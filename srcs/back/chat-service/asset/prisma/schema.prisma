datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output = "./prisma_client"
}

model msg {
  id Int @id @unique @default(autoincrement())
  userID Int
  channel Int
  createdAt DateTime @default(now())
  text String
}

model channel {
  id Int @id @unique @default(autoincrement())
  hash String @unique
}